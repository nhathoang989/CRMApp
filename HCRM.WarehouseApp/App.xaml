<Application x:Class="HCRM.WarehouseApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:HCRM.WarehouseApp"
             >
    <Application.Resources>
       
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDic\MainDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <sys:Double x:Key="ScreenWidth">760</sys:Double>
            <sys:Double x:Key="ScreenHeight">600</sys:Double>
        </ResourceDictionary>        

    </Application.Resources>
</Application>
